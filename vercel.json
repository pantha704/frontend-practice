{
  "version": 2,
  "builds": [
    {
      "src": "qr-code-component-main/**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/qr-code",
      "dest": "/qr-code-component-main/index.html"
    },
    {
      "src": "/qr-code/question",
      "dest": "/qr-code-component-main/question.html"
    },
    {
      "src": "/qr-code/solution",
      "dest": "/qr-code-component-main/solution.html"
    }
  ]
}
